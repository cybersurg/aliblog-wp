{
  "name": "ali-child",
  "private": true,
  "scripts": {
    "build": "tailwindcss -i src/css/app.css -o dist/app.css --minify && esbuild src/js/app.js --bundle --outfile=dist/app.js --minify",
    "dev": "tailwindcss -i src/css/app.css -o dist/app.css --watch & esbuild src/js/app.js --bundle --watch --outfile=dist/app.js"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "tailwindcss": "^3.4.9"
  }
}
